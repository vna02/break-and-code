// Break and Code question no.3

//Library
#include <iostream>
using namespace std;

//Main function
int main(){

	//Declare variable 
	int i=0;
	
	//An infinite loop, where we check for those numbers which are squares ie.x.
	while(i>=0)
		{
		int x=i*i;
		
			//A finite loop, where we know the zth number is less than the perfect square x.
			int z=0;
			while(z<=x)
			{   
				//A condition to check whether that number is the sum of the first z numbers is equal to the number x.
				if (x== (z*(z+1))/2)
					{
					//Print the number x
					cout<<x<<endl;
					}
			z++;
			}
		i++;
		}

return 0;
}
